# Core OpenAI configuration
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-mini
LLM_PROMPT_VERSION=2025-09-19-form-orchestrator

# Feature flags
ENABLE_LLM_ORCHESTRATION=true
LLM_ROLLOUT_PERCENTAGE=100
ENABLE_EVAL_LOGGING=true
ENABLE_PROMPT_INTEL=true
ENABLE_PERSONALIZATION=true
ENABLE_LABELING_REVIEW=false

# Client-side defaults
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_LLM_STRATEGY=llm
NEXT_PUBLIC_DEBUG=true

# Evaluation logging
EVAL_LOG_DIR=eval/logs
AIRTABLE_API_KEY=
AIRTABLE_BASE_ID=
AIRTABLE_TABLE_NAME=llm_decisions

# Optional: adjust Bun/Next runtime behaviour
AI_SDK_LOG_WARNINGS=false
